<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="AnchorPane" prefHeight="480.0" prefWidth="640.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.horrorho.liquiddonkey.gui.controller.AuthenticationController">
    <stylesheets>
        <URL value="@/styles/Styles.css" />
    </stylesheets>
   <center>
      <Accordion fx:id="accordion" layoutX="14.0" layoutY="14.0" prefHeight="288.0" prefWidth="298.0">
        <panes>
          <TitledPane fx:id="authentication" text="AppleId Password">
               <content>
                  <BorderPane>
                     <top>
                        <VBox alignment="CENTER" BorderPane.alignment="CENTER">
                           <children>
                              <ImageView pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/lqea.png" />
                                 </image>
                              </ImageView>
                              <Label alignment="CENTER" prefHeight="45.0" prefWidth="322.0" text="LiquidDonkey">
                                 <font>
                                    <Font size="26.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                     </top>
                     <bottom>
                        <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                           <children>
                              <Button fx:id="goAppleIdPassword" mnemonicParsing="false" onAction="#handleGoAppleIdPassword" text="Go" />
                           </children>
                        </HBox>
                     </bottom>
                     <center>
                        <HBox alignment="CENTER" BorderPane.alignment="CENTER">
                           <children>
                              <VBox alignment="CENTER" prefWidth="240.0" spacing="16.0">
                                 <children>
                                    <TextField fx:id="appleId" onAction="#handleAppleId" promptText="AppleId" />
                                    <PasswordField fx:id="password" onAction="#handlePassword" promptText="Password" />
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                     </center>
                  </BorderPane>
               </content>
          </TitledPane>
            <TitledPane prefHeight="200.0" prefWidth="200.0" text="Authorization Token">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <top>
                        <VBox alignment="CENTER" BorderPane.alignment="CENTER">
                           <children>
                              <ImageView pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/lqea.png" />
                                 </image>
                              </ImageView>
                              <Label text="LiquidDonkey">
                                 <font>
                                    <Font size="26.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                     </top>
                     <bottom>
                        <HBox alignment="BOTTOM_RIGHT" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                           <children>
                              <Button fx:id="goAuthToken" mnemonicParsing="false" onAction="#handleGoAuthToken" text="Go" />
                           </children>
                        </HBox>
                     </bottom>
                     <center>
                        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                           <children>
                              <VBox alignment="CENTER" prefWidth="480.0">
                                 <children>
                                    <TextField fx:id="authToken" onAction="#handleAuthToken" promptText="Authorization Token" />
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                     </center>
                  </BorderPane>
               </content>
            </TitledPane>
          <TitledPane fx:id="advanced" text="Advanced">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <top>
                        <VBox alignment="CENTER" BorderPane.alignment="CENTER">
                           <children>
                              <ImageView pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@../images/lqea.png" />
                                 </image>
                              </ImageView>
                           </children>
                        </VBox>
                     </top>
                     <center>
                        <GridPane BorderPane.alignment="CENTER">
                          <columnConstraints>
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <children>
                              <VBox spacing="16.0" GridPane.columnIndex="1">
                                 <children>
                                    <CheckBox mnemonicParsing="false" text="Persistent" />
                                    <CheckBox mnemonicParsing="false" text="Aggressive" />
                                    <CheckBox mnemonicParsing="false" text="Relax SSL" />
                                 </children>
                                 <padding>
                                    <Insets top="16.0" />
                                 </padding>
                              </VBox>
                              <VBox spacing="16.0" GridPane.rowIndex="1">
                                 <children>
                                    <Label text="Threads">
                                       <graphic>
                                          <ChoiceBox />
                                       </graphic>
                                    </Label>
                                 </children>
                                 <padding>
                                    <Insets top="16.0" />
                                 </padding>
                              </VBox>
                              <VBox prefHeight="200.0" prefWidth="100.0" spacing="16.0">
                                 <children>
                                    <CheckBox mnemonicParsing="false" text="Combine" />
                                    <CheckBox mnemonicParsing="false" text="Force" />
                                 </children>
                                 <padding>
                                    <Insets top="16.0" />
                                 </padding>
                              </VBox>
                           </children>
                        </GridPane>
                     </center>
                     <bottom>
                        <Button mnemonicParsing="false" text="Defaults" BorderPane.alignment="BOTTOM_LEFT" />
                     </bottom>
                  </BorderPane>
               </content>
          </TitledPane>
        </panes>
      </Accordion>
   </center>
</BorderPane>
